  "dependencies": {
    "@zakstam/codex-runtime": "workspace:*",
    "@zakstam/codex-runtime-bridge-tauri": "workspace:*"
  },
  "devDependencies": {
    "@typescript/native-preview": "7.0.0-dev.20260221.1",
    "@types/node": "^22.13.1",
    "tsx": "^4.20.3",
    "typescript": "^5.7.3"
  }
}
