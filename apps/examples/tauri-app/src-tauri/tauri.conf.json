{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Codex Local Tauri Example",
  "version": "0.1.0",
  "identifier": "dev.zakstam.codex.local.example",
  "build": {
    "beforeDevCommand": "pnpm run dev:web",
    "beforeBuildCommand": "vite build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Codex Local Tauri Example",
        "width": 1320,
        "height": 880,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": {
      "../dist-node/bridge-helper.js": "bridge-helper.js"
    }
  }
}
