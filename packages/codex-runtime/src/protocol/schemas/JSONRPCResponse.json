{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSONRPCResponse",
  "description": "A successful (non-error) response to a request.",
  "type": "object",
  "required": [
    "id",
    "result"
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/RequestId"
    },
    "result": true
  },
  "definitions": {
    "RequestId": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "integer",
          "format": "int64"
        }
      ]
    }
  }
}