    "roots"
  ],
  "properties": {
    "cancellationToken": {
      "type": [
        "string",
        "null"
      ]
    },
    "query": {
      "type": "string"
    },
    "roots": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}